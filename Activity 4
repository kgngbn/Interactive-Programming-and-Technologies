void loop()
{
  Serial.print("SELECT MUSIC\n");
  Serial.print("Please enter 1(Tetris), 2(MarioBros), 3(Pacman), 4(JigglyPuff), or 5(KeyboardCat).\n");
  while (Serial.available() == 0)
  {
  }
  select = Serial.parseInt();
  switch (select) {
  case 1:
      Music1();
    break;
  case 2:
    Music2();
    break;
  case 3:
    Music3();
    break;
  case 4:
    Music4();
    break;
  case 5:
    Music5();
    break;
  }
  }
