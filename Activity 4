tone(buzzer, 740); delay(100); noTone(buzzer); //NOTE_FS5
tone(buzzer, 659); delay(100); noTone(buzzer); //NOTE_E5
tone(buzzer, 740); delay(100); noTone(buzzer); //NOTE_FS5
tone(buzzer, 587); delay(100); noTone(buzzer); //NOTE_D5
tone(buzzer, 587); delay(100); noTone(buzzer); //NOTE_D5
tone(buzzer, 880); delay(100); noTone(buzzer); //NOTE_A5
tone(buzzer, 740); delay(100); noTone(buzzer); //NOTE_FS5
tone(buzzer, 587); delay(100); noTone(buzzer); //NOTE_D5
tone(buzzer, 659); delay(100); noTone(buzzer); //NOTE_E5
tone(buzzer, 740); delay(100); noTone(buzzer); //NOTE_FS5
tone(buzzer, 784); delay(100); noTone(buzzer); //NOTE_G5
tone(buzzer, 740); delay(100); noTone(buzzer); //NOTE_FS5
}

